{
  "extends": "airbnb-base",
  "rules": {
    "prefer-destructuring": ["error", {
      "VariableDeclarator": {
        "array": true,
        "object": true
      },
      "AssignmentExpression": {
        "array": false,
        "object": false
      }
    }, {
      "enforceForRenamedProperties": false
    }]
  },
  "env": {
    "es6": true,
    "browser": true,
    "jest": true
  }
}
